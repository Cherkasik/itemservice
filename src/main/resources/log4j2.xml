<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" name="XMLConfig" packages="org.apache.logging.log4j">
  <Appenders>
    <File name="File" fileName="itemService.log" advertiseURI="file://itemService.log" advertise="true">
      <PatternLayout pattern="%d %-5p %c{1}:%L - %m%n"/>
    </File>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="File"/>
    </Root>
  </Loggers>
</Configuration>